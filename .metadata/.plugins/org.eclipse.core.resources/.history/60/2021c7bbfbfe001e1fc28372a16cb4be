#include "main.h"

void menu_task(void * param)
{
	while(1)
	{

	}
}

void cmd_task(void * param)
{
	BaseType_t ret;
	command_t cmd;
	while(1)
	{
		ret = xTaskNotifyWait(0,0,NULL,portMAX_DELAY);
		if(ret == pdTRUE)
		{
			process_cmd(&cmd);
		}
	}
}

